{
  "kind": "collectionType",
  "collectionName": "criar_servicos",
  "info": {
    "singularName": "criar-servico",
    "pluralName": "criar-servicos",
    "displayName": "Criar_servico",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Titulo": {
      "type": "string"
    },
    "Descricao": {
      "type": "richtext"
    },
    "Inicio": {
      "type": "date",
      "default": "2022-04-01"
    },
    "Imagens": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "tipos_servicos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tipos-servico.tipos-servico",
      "mappedBy": "criar_servico"
    },
    "empresa": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::empresa.empresa",
      "inversedBy": "criar_servicos"
    },
    "Valor": {
      "type": "decimal"
    },
    "Info_adicionais": {
      "type": "text"
    },
    "Gerente_Empresa": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "criar_servicos"
    },
    "colaborador": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::colaborador.colaborador"
    },
    "Status_Servicos": {
      "type": "enumeration",
      "enum": [
        "Aguardando",
        "Iniciado",
        "Finalizado"
      ],
      "default": "Aguardando"
    },
    "Pagamento": {
      "type": "enumeration",
      "enum": [
        "Aguardando",
        "Pago"
      ],
      "default": "Aguardando"
    },
    "colaboradors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::colaborador.colaborador",
      "inversedBy": "Servicos_Relacionados"
    }
  }
}
